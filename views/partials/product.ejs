
<div class="col_1_of_3 span_1_of_3">
    
        <div class="view view-fifth">
            <div class="top_box">
                <a href="product_details.ejs">
                <h3 class="m_1"><%= title %></h3>
                <p class="m_2"><%= subtitle %></p>
                <div class="grid_img">
                    <div class="css3">
                        <img src="<%= imgsrc %>" alt="<%= imgalt %>"/>
                    </div>
                </div>
                <div class="price"><%= price %></div>
            </div>
        </div>
        <!-- Rating / Add to Cart Btn -->
        <div class="bottomCard">
            <span class="rating">
                <!-- 
                    Inputs for class="< star >" should be one of the following:
                        rating-star1 = Has star in place
                        rating-star = No star in place
                -->
                <input type="radio" class="rating-input" id="rating-input-1-5" name="rating-input-1">
                <label for="rating-input-1-5" class="<%= star1 %>"></label>
                <input type="radio" class="rating-input" id="rating-input-1-4" name="rating-input-1">
                <label for="rating-input-1-4" class="<%= star2 %>"></label>
                <input type="radio" class="rating-input" id="rating-input-1-3" name="rating-input-1">
                <label for="rating-input-1-3" class="<%= star3 %>"></label>
                <input type="radio" class="rating-input" id="rating-input-1-2" name="rating-input-1">
                <label for="rating-input-1-2" class="<%= star4 %>"></label>
                <input type="radio" class="rating-input" id="rating-input-1-1" name="rating-input-1">
                <label for="rating-input-1-1" class="<%= star5 %>"></label>&nbsp;
                <%= rating %>
            </span>
            <!-- Add to cart button -->
            <ul class="list">
                <li>
                    <ul class="icon1 sub-icon1 profile_img">
                        <li><a class="active-icon c1" href="#">Add To Cart</a></li>
                    </ul>
                </li>
            </ul>
        </div>
    </a>
</div>
